<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.image.Image?>
<GridPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity"
          minWidth="-Infinity" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="controllers.administrator.AccountScreenController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="30.0" minWidth="30.0" prefWidth="30.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="30.0" minWidth="30.0" prefWidth="30.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="10.0" minHeight="10.0" prefHeight="10.0" valignment="CENTER" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="1.7976931348623157E308" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="10.0" minHeight="10.0" prefHeight="10.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <HBox alignment="CENTER_LEFT" GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.rowIndex="1" GridPane.valignment="CENTER">
            <children>
                <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="icons/user_icon.png" />
                    </image>
                </ImageView>
                <Label maxWidth="10" prefWidth="10" text="  " />
                <Label maxHeight="40.0" maxWidth="1.7976931348623157E308" minHeight="40.0" prefHeight="40.0"
                       text="ACCOUNTS LIST" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="22.0"/>
                    </font>
                </Label>
            </children>
        </HBox>
        <GridPane styleClass="general-content" stylesheets="/styles/NewGridPaneStyle.css" GridPane.columnIndex="1"
                  GridPane.rowIndex="3">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="45.0" minHeight="45.0" prefHeight="45.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="15.0" minHeight="15.0" prefHeight="15.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="1.7976931348623157E308" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <TableView fx:id="accountListDataTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="1200.0"
                           styleClass="table-view" stylesheets="/styles/NewTableViewStyle.css" GridPane.rowIndex="2">
                    <columns>
                        <TableColumn fx:id="uniqueIdColumn" minWidth="-1.0" prefWidth="100.0" text="ID"/>
                        <TableColumn fx:id="mailColumn" minWidth="-1.0" prefWidth="200.0" text="Adres email"/>
                        <TableColumn fx:id="loginColumn" minWidth="-1.0" prefWidth="150.0" text="Login"/>
                        <TableColumn fx:id="accountTypeColumn" minWidth="-1.0" prefWidth="100.0" text="Typ konta"/>
                        <TableColumn fx:id="passwordExpiredDateColumn" minWidth="-1.0" prefWidth="100.0" text="Data wygśnięcia hasła"/>
                        <TableColumn fx:id="userDataColumn" minWidth="-1.0" prefWidth="250.0" text="Dane użytkownika"/>
                        <TableColumn fx:id="creationDateColumn" minWidth="-1.0" prefWidth="150.0" text="Data utworzenia"/>
                        <TableColumn fx:id="registrationIndicatorColumn" minWidth="-1.0" prefWidth="100.0" text="Wsk. zatwierdzenia"/>
                        <TableColumn fx:id="activeIndicatorColumn" minWidth="-1.0" prefWidth="100.0" text="Wsk. aktywności"/>
                    </columns>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </TableView>
                <GridPane>
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308"/>
                        <ColumnConstraints/>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="15.0" minWidth="15.0" prefWidth="15.0"/>
                        <ColumnConstraints/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints maxHeight="15.0" minHeight="15.0" prefHeight="15.0" vgrow="SOMETIMES"/>
                        <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <Label text="Rodzaj" GridPane.columnIndex="1"/>
                        <Label text="Status" GridPane.columnIndex="3"/>
                        <Spinner fx:id="statusSpinner" styleClass="split-arrows-horizontal" GridPane.columnIndex="3"
                                 GridPane.rowIndex="1">
                            <cursor>
                                <Cursor fx:constant="HAND"/>
                            </cursor>
                        </Spinner>
                        <Spinner fx:id="moduleSpinner" styleClass="split-arrows-horizontal" GridPane.columnIndex="1"
                                 GridPane.rowIndex="1">
                            <cursor>
                                <Cursor fx:constant="HAND"/>
                            </cursor>
                        </Spinner>
                    </children>
                </GridPane>
            </children>
        </GridPane>
        <GridPane GridPane.columnIndex="1" GridPane.rowIndex="5">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="15.0" minWidth="15.0" prefWidth="15.0"/>
                <ColumnConstraints hgrow="SOMETIMES"/>
                <ColumnConstraints hgrow="ALWAYS" maxWidth="15.0" minWidth="15.0" prefWidth="15.0"/>
                <ColumnConstraints hgrow="SOMETIMES"/>
                <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <HBox>
                    <children>
                        <Button fx:id="addButton" maxHeight="30.0" maxWidth="30.0" minHeight="30.0" minWidth="30.0"
                                mnemonicParsing="false" onAction="#onAddKey" prefHeight="30.0" prefWidth="30.0"
                                styleClass="button" stylesheets="/styles/NewButtonStyle.css" text="A">
                            <cursor>
                                <Cursor fx:constant="HAND"/>
                            </cursor>
                        </Button>
                        <Button fx:id="addLabelButton" maxHeight="30.0" minHeight="30.0" mnemonicParsing="false"
                                onAction="#onAddKey" prefHeight="30.0" styleClass="label-button"
                                stylesheets="/styles/NewButtonStyle.css" text="ADD">
                            <cursor>
                                <Cursor fx:constant="HAND"/>
                            </cursor>
                        </Button>
                    </children>
                </HBox>
                <HBox GridPane.columnIndex="2">
                    <children>
                        <Button fx:id="editButton" maxHeight="30.0" maxWidth="30.0" minHeight="30.0" minWidth="30.0"
                                mnemonicParsing="false" onAction="#onEditKey" prefHeight="30.0" prefWidth="30.0"
                                styleClass="button" stylesheets="/styles/NewButtonStyle.css" text="E">
                            <cursor>
                                <Cursor fx:constant="HAND"/>
                            </cursor>
                        </Button>
                        <Button fx:id="editLabelButton" maxHeight="30.0" minHeight="30.0" mnemonicParsing="false"
                                onAction="#onEditKey" prefHeight="30.0" styleClass="label-button"
                                stylesheets="/styles/NewButtonStyle.css" text="EDIT">
                            <cursor>
                                <Cursor fx:constant="HAND"/>
                            </cursor>
                        </Button>
                    </children>
                </HBox>
                <HBox GridPane.columnIndex="4">
                    <children>
                        <Button fx:id="deleteButton" maxHeight="30.0" maxWidth="30.0" minHeight="30.0" minWidth="30.0"
                                mnemonicParsing="false" onAction="#onDeleteKey" prefHeight="30.0" prefWidth="30.0"
                                styleClass="button" stylesheets="/styles/NewButtonStyle.css" text="D">
                            <cursor>
                                <Cursor fx:constant="HAND"/>
                            </cursor>
                        </Button>
                        <Button fx:id="deleteLabelButton" maxHeight="30.0" minHeight="30.0" mnemonicParsing="false"
                                onAction="#onDeleteKey" prefHeight="30.0" styleClass="label-button"
                                stylesheets="/styles/NewButtonStyle.css" text="DELETE">
                            <cursor>
                                <Cursor fx:constant="HAND"/>
                            </cursor>
                        </Button>
                    </children>
                </HBox>
            </children>
        </GridPane>
    </children>
</GridPane>
