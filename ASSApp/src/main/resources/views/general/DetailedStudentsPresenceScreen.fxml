<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.general.DetailedStudentsPresenceController" styleClass="basic-background" stylesheets="/styles/BackgroundStyle.css">
   <children>
      <VBox layoutX="14.0" layoutY="14.0" spacing="10.0">
         <children>
            <Label alignment="CENTER" text="Szczegółowe dane o studentach na danych zajęciach" styleClass="title" stylesheets="/styles/LabelStyle.css">
               <font>
                  <Font name="Cambria Bold" size="16.0" />
               </font>
            </Label>
            <TableView fx:id="studentsTable" prefHeight="440.0" prefWidth="770.0" styleClass="table-view" stylesheets="/styles/TableViewStyle.css">
               <columns>
                   <TableColumn fx:id="cardIdColumn" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="ID karty" />
                   <TableColumn fx:id="studentIdColumn" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Indeks studenta" />
                   <TableColumn fx:id="nameColumn" maxWidth="100.0" minWidth="59.0" prefWidth="63.0" text="Imię" />
                   <TableColumn fx:id="surnameColumn" maxWidth="141.0" minWidth="93.0" prefWidth="93.0" text="Nazwisko" />
                   <TableColumn fx:id="facultyColumn" maxWidth="144.0" minWidth="89.0" prefWidth="89.0" text="Wydział" />
                   <TableColumn fx:id="deanGroupColumn" maxWidth="125.0" minWidth="65.0" prefWidth="123.0" text="Grupa dziekańska" />
                   <TableColumn fx:id="emailColumn" maxWidth="145.0" minWidth="36.0" prefWidth="122.0" text="Adres email" />
                   <TableColumn fx:id="wasLateColumn" maxWidth="145.0" minWidth="0.0" prefWidth="79.0" text="Spóźniony" />
               </columns>
            </TableView>
         </children>
      </VBox>
   </children>
</AnchorPane>
